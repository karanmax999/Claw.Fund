POSITION_SIZE=0.1

# Wallet (mock for now)
WALLET_ADDRESS=0xMOCK_WALLET_ADDRESS
PRIVATE_KEY=0xMOCK_PRIVATE_KEY

# Logging
LOG_LEVEL=debug
LOG_DIR=./logs
